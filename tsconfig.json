{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "allowJs": true,
    "outDir": "build",
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "esModuleInterop": true,
    // "rootDirs": [
    //   "./src"
    // ],
    "paths": {
      "@instamation": ["instamation/index"],
      "@instamation/*": ["instamation/*"],
      "@helpers/*": ["helpers/*"],
      "@selectors": ["selectors"],
      "@config": ["config"]
    }
  }
}